Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

urxvt.font:             xft:Bitstream Vera Sans Mono:pixelsize=15:autohint=true:antialias=false
urxvt.boldFont:         xft:Bitstream Vera Sans Mono:bold:size=15:autohint=true:antialias=false
urxvt.italicFont:       xft:Bitstream Vera Sans Mono:italic:size=15:autohint=true:antialias=false
urxvt.termName:         rxvt
urxvt.background:       #000000
urxvt.foreground:       #cccccc
urxvt.scrollBar:        false
urxvt.perl-lib:         /usr/lib/urxvt/perl/
urxvt.perl-ext-common:  default,matcher,font-size
URxvt.url-launcher:      /usr/bin/google-chrome
URxvt.matcher.button:   1
urxvt.fading:           30
urxvt.fadeColor:        #000000

URxvt.dynamicColors:    on


urxvt.depth:        32

URxvt.saveLines:        1000

URxvt.transparent:   false
URxvt.tintColor:     black
#URxvt.shading:       18

URxvt.keysym.C-Up:     perl:font-size:increase
URxvt.keysym.C-Down:   perl:font-size:decrease
URxvt.keysym.C-S-Up:   perl:font-size:incglobal
URxvt.keysym.C-S-Down: perl:font-size:decglobal

URxvt.iso14755: false
URxvt.iso14755_52: false

